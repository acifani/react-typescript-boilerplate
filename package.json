    "@babel/preset-typescript": "^7.0.0-rc.3",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0-beta.4",
